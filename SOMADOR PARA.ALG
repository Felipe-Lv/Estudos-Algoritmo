Algoritmo "somador para"
Var
   contador, numero, soma : inteiro
Inicio
      soma <- 0
      
      para contador <- 1 ate 5 faca
           escreval("Digite um valor")
           leia(numero)
           soma <- soma + numero
      fimPara
      escreva("A soma dos valores é: " ,soma)

Fimalgoritmo