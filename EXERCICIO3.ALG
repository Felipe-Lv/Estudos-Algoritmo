Algoritmo "semnome"
Var
   nome1, nome2: caractere
   salario1: inteiro
Inicio
          escreva("Qual o seu nome? ")
          leia(nome1)
          escreva("Qual o seu sobrenome? ")
          leia(nome2)
          escreval("Informe o salário: ")
          leia(salario1)
          escreva("O funcionário ", nome1, " ", nome2, " tem um salário de R$" ,salario1, " em Junho.")
Fimalgoritmo
