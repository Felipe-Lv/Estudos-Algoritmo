Algoritmo "imposto"
//  
//  
Var
   valor1, imposto1, imposto2,imposto3:real
Inicio
      escreva("Qual o valor do produto? ")
      leia(valor1)
      imposto1 <- 60
      imposto2 <- 100
      imposto3 <- (valor1*imposto1)/imposto2)
      escreval("O valor do imposto sera de:", imposto3)
      escreva("O valor real do produto:", valor1-imposto3)
Fimalgoritmo