Algoritmo "contador"
Var
   contar, valor, salto: inteiro
Inicio
      contar <-0
      escreva("Quer contar até quanto? ")
      leia(valor)
      escreva("Qual valor ser pulado? ")
      leia(salto)
      Enquanto (contar <= valor) FACA
         escreval("contador: ",contar)
       contar <- contar + salto
      fimEnquanto
      escreval("Terminei de contar")
Fimalgoritmo
